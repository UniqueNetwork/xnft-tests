{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ESNext"],
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"module": "ES2022",
		"sourceMap": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
	},
	"include": [
		"./src/*",
		"./src/unique-chains/*",
		"./src/acala-chains/*",
	],
}
